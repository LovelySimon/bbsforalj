<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alj.mapper.UserInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.alj.pojo.UserInfo">
        <id column="user_id" property="userId" />
        <result column="nick_name" property="nickName" />
        <result column="email" property="email" />
        <result column="password" property="password" />
        <result column="sex" property="sex" />
        <result column="person_description" property="personDescription" />
        <result column="join_time" property="joinTime" />
        <result column="last_login_time" property="lastLoginTime" />
        <result column="last_login_ip" property="lastLoginIp" />
        <result column="last_login_ip_address" property="lastLoginIpAddress" />
        <result column="total_integral" property="totalIntegral" />
        <result column="current_integral" property="currentIntegral" />
        <result column="status" property="status" />
        <result column="personality" property="personality"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, nick_name, email, password, sex, person_description, join_time, last_login_time, last_login_ip, last_login_ip_address, total_integral, current_integral, status, personality
    </sql>

</mapper>
